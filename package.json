{
  "name": "xmm-node",
  "version": "0.3.2",
  "author": "Joseph Larralde",
  "license": "GPL-3.0",
  "description": "node wrapper for the XMM library",
  "keywords": [
    "xmm",
    "machine learning",
    "gmm",
    "hmm",
    "regression"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ircam-RnD/xmm-node.git"
  },
  "bundledDependencies":["node-pre-gyp"],
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build > log.txt 2>&1",
    "docs": "jsdoc2md -t docs/README.hbs docs/documentation.js > README.md",
    "test": "babel-tape-runner \"test/*_tests.js\""
  },
  "dependencies": {
    "nan": "^2.3.5",
    "node-pre-gyp": "^0.6.36"
  },
  "binary": {
    "module_name": "xmm-node",
    "module_path": "./build/bin/{configuration}/{node_abi}-{platform}-{arch}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://github.com/Ircam-RnD/xmm-node/releases/download/",
    "remote_path": "{version}"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-tape-runner": "^2.0.1",
    "jsdoc-template": "ircam-jstools/jsdoc-template",
    "jsdoc-to-markdown": "^1.3.7",
    "tape": "^4.6.0",
    "xmm-client": "ircam-rnd/xmm-client"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
