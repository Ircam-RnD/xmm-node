{	
	"source": {
    "include": [
    	"docs/README_INTRO.md",
    	"docs/documentation.js"
    ],
    "exclude": [ "xmm" ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": [
		"plugins/markdown"
	],
	"opts": {
		"destination": "./docs",
		"encoding": "utf8",
		"template": "node_modules/jsdoc-template"
	},
	"templates": {
		"default": {
			"useLongnameInNav": true
		}
	}
}
